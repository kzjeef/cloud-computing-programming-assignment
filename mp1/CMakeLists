project(MP1)

set(CMAKE_CXX_FLAGS "${CMAKE_CXX_FLAGS} -std=c++11 -Wall -g")

set(SOURCE_FILES Application.cpp Application.h Member.h Log.h Params.h Member.h EmulNet.h Queue.h
